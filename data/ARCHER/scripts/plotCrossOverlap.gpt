# plot Overlap 
set terminal png notransparent rounded font 24
set output "overlap.png"
set multiplot
set log y
set key left
set xlabel "Message size (KBytes)"
set ylabel "Time (s)"
p "overlap/cross" u ($2*8/1024):3 w lp title "Total", "" u ($2*8/1024):4 w lp title "Computation", "" u ($2*8/1024):5 w lp title "Communication"
set origin 0.45,0.1
set size 0.45,0.4
set xrange[0.5:2.5]
set yrange[0.005:0.12]
unset xlabel
unset ylabel
unset label
#set xtics (1,2,4,8,16,32,64,128,256,512,1024,)
#set xtics (0.5,1.5,2.5,3.5)
set mxtics
p "overlap/cross" u ($2*8/1024):3 w lp notitle, "" u ($2*8/1024):4 w lp notitle, "" u ($2*8/1024):5 w lp notitle
unset multiplot
unset output


